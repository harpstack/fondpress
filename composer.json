{
    "name": "harpstack/fondpress",
    "description": "A Slim-based content management system",
    "keywords": [
        "fondpress",
        "slim",
        "cms",
        "php"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Harper Zhang",
            "email": "harper.zhang@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "slim/php-view": "^3.1",
        "slim/twig-view": "^3.3",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.3",
        "symfony/console": "^5.4",
        "symfony/polyfill-uuid": "^1.24",
        "cakephp/database": "^4.3",
        "cakephp/validation": "^4.3",
        "laminas/laminas-config": "^3.7",
        "fullpipe/twig-webpack-extension": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
